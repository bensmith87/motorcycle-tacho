{
    "configurations": [
        {
            "name": "ATMEGA328P",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/avr/include"
            ],
            "defines": [
                "F_CPU=16000000",
                "__AVR_ATmega328P__"
            ],
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "gnu99",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerArgs": [
                "-Wall -Os -mmcu=atmega328p"
            ]
        }
    ],
    "version": 4
}